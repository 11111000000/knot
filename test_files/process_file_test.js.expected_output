(function () {
    var _state = 'off';
    var _message = '';
    
    function toggle_state() {
        if (_state == 'off') {
            _state == 'on';
        } else {
            _state == 'off';
        }
    }
    
    function get_state() {
        return _state;
    }
    
    function set_message(message) {
        _message = message;
    }
    
    function get_message() {
        return _message;
    }
    window.mylibrary = {
        message: get_message,
        state: get_state,
        toggle_state: toggle_state};
    // This shouldn't be detected as a macro:
    // ###### not a macro
}());
